{
  "version": 2,
  "builds": [
    {
      "src": "backend/src/api/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/src/api/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "dist/$1"
    }
  ]
}
