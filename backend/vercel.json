{
  "functions": {
    "backend/src/api/index.js": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/src/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/backend/dist/$1"
    }
  ],
  "outputDirectory": "backend/dist"
}